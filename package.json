{
  "name": "boilerplate",
  "version": "0.2.0",
  "private": true,
  "description": "Projeto padrão node.js para novas aplicações",
  "main": "bin/index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "node bin/index.js",
    "start:dev": "nest start --exec \"ts-node -r dotenv/config\" --watch",
    "prebuild": "npx del bin",
    "build": "nest build",
    "test": "jest test/unit --runInBand --forceExit",
    "test:watch": "jest test/unit --watch",
    "test:coverage": "jest test/unit --coverage --runInBand --forceExit",
    "restart:dockercompose": "npm run stop:docker && npm run start:docker",
    "lint": "npm run lint:format && npm run lint:style",
    "lint:fix": "npm run lint:format:fix && npm run lint:style:fix",
    "lint:format": "prettier --check '{src,test}/**/*.ts'",
    "lint:format:fix": "prettier --write '{src,test}/**/*.ts'",
    "lint:style": "eslint '**/*.ts'",
    "lint:style:fix": "eslint '**/*.ts' --fix",
    "prepare": "husky install"
  },
  "author": "Douglas Campos",
  "license": "ISC",
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/testing": "^9.2.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/compression": "^1.7.0",
    "@types/ioredis": "^4.16.7",
    "@types/jest": "^29.2.2",
    "@types/mocha": "^7.0.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.18.33",
    "@types/read-pkg": "^4.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@types/superagent": "^4.1.15",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "del-cli": "^3.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-callslike": "^0.2.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@codibre/fluent-iterable": "^1.24.1",
    "@fastify/static": "^6.5.0",
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/platform-fastify": "^9.2.0",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/swagger": "^6.1.3",
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/context-async-hooks": "^1.8.0",
    "@remembered/redis": "^1.1.8",
    "amadeus": "^7.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "for-emit-of": "^1.4.0",
    "ioredis": "^4.17.3",
    "ms": "^2.1.3",
    "qs": "^6.11.0",
    "read-pkg": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "ts-base-http-client": "^0.1.0",
    "un": "^0.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.2",
    "winston-context-logger": "^0.1.9",
    "winston-context-logger-open-telemetry": "^0.1.6"
  }
}
