{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      
        {
            "type": "node",
            "request": "launch",
            "name": "Launch program",
            "skipFiles": [
              "<node_internals>/**",
              "${workspaceRoot}/node_modules/augmentative-iterable",
              "${workspaceRoot}/node_modules/@codibre/fluent-iterable",
              "${workspaceRoot}/node_modules/**/*augmentative-iterable*",
              "${workspaceRoot}/node_modules/**/*fluent-iterable*"
            ],
            "cwd": "${workspaceRoot}",
            "envFile": "${workspaceFolder}/.env",
            "runtimeExecutable": "${env:NVM_HOME}/nvm-exec",
            "runtimeArgs": ["node", "-r", "tsconfig-paths/register", "-r", "ts-node/register"],
            "args": ["${workspaceRoot}/src/index.ts"],
            "outputCapture": "std",
          },
          {
            "type": "node",
            "request": "launch",
            "name": "Launch Windows",
            "skipFiles": [
              "<node_internals>/**",
              "${workspaceRoot}/node_modules/augmentative-iterable",
              "${workspaceRoot}/node_modules/@codibre/fluent-iterable",
              "${workspaceRoot}/node_modules/**/*augmentative-iterable*",
              "${workspaceRoot}/node_modules/**/*fluent-iterable*"
            ],
            "cwd": "${workspaceRoot}",
            "envFile": "${workspaceFolder}/.env",
            "runtimeArgs": ["-r", "tsconfig-paths/register", "-r", "ts-node/register"],
            "runtimeVersion": "18.12.1",
            "args": ["${workspaceRoot}/src/index.ts"],
            "outputCapture": "std",
          },
          {
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "${env:HOME}/.nvm/nvm-exec",
            "name": "Tests",
            "skipFiles": ["<node_internals>/**"],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": ["node", "-r", "tsconfig-paths/register", "-r", "ts-node/register"],
            "args": [
              "${workspaceRoot}/node_modules/jest/bin/jest",
              "test/unit",
              "--runInBand"
            ],
            "outputCapture": "std"
          }
    ]
}